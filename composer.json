{
    "name": "darthsoup/php-whmcs-api",
    "description": "php whmcs api client",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Krummnacker",
            "email": "kevin.krummnacker@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "psr/http-client-implementation": "^1.0",
        "psr/http-factory-implementation": "^1.0",
        "psr/http-message": "^1.0",
        "php-http/client-common": "^2.3",
        "php-http/discovery": "^1.13",
        "php-http/httplug": "^2.2",
        "symfony/options-resolver": "^4.0 || ^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5.14 || ^9.5.1",
        "guzzlehttp/guzzle": "^7.0",
        "http-interop/http-factory-guzzle": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "DarthSoup\\WhmcsApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DarthSoup\\Tests\\WhmcsApi\\": "tests/"
        }
    }
}
